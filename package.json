{
  "name": "aeon",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Event store for AWS DynamoDB",
  "author": {
    "name": "Brian Cavalier",
    "email": "brian@briancavalier.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/briancavalier/aeon.git"
  },
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./aws-cdk/": {
      "import": "./dist/esm/aws-cdk/index.js",
      "require": "./dist/cjs/aws-cdk/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project ./tsconfig.build-esm.json --incremental --tsBuildInfoFile dist/esm/.tsbuildinfo",
    "build:cjs": "tsc --project ./tsconfig.build-cjs.json --incremental --tsBuildInfoFile dist/cjs/.tsbuildinfo",
    "test": "npx tsx --experimental-test-coverage --test-coverage-exclude **/*.test.ts --test src/**/*.test.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.724.0",
    "@aws-sdk/client-eventbridge": "^3.723.0",
    "@aws-sdk/client-sns": "^3.723.0",
    "@aws-sdk/lib-dynamodb": "^3.731.1",
    "@types/aws-lambda": "^8.10.147",
    "@types/node": "^22.10.5",
    "aws-cdk": "^2.176.0",
    "aws-cdk-lib": "^2.174.1",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.4.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "ulid": "^2.3.0"
  }
}
